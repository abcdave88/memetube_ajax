<div class="content">
  
  <h3>All videos</h3>

  <div class="video-list"
   <% @videos.each do |video| %>
          <tr>
            <td>
              <li><a href="/videos/<%= video['id'] %>">
                <%= [video['artist'], video['title']].join(" - ") %>
              </a></li>
            </td>
          </tr>
        <% end %>
  </div>  
</div>
      