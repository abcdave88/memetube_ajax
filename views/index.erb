<h2>Home Page</h2>
<h3>All videos</h3>

 <% @videos.each do |video| %>
        <tr>
          <td>
            <a href="/videos/<%= video['id'] %>">
              <%= [video['artist'], video['title']].join(" - ") %>
            </a>
          </td>
        </tr>
      <% end %>

      <iframe width="420" height="315" src="https://www.youtube.com/embed/rS22QSZmZoU" frameborder="0" allowfullscreen></iframe>